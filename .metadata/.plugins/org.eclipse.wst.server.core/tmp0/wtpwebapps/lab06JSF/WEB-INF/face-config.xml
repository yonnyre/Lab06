<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<navigation-rule>
	    <from-view-id>04/actionNavigation.xhtml</from-view-id>
	    <navigation-case>
		<from-action>#{navigation2Ctrl.processPage1}</from-action>
		<from-outcome>page</from-outcome>
		<to-view-id>04/page1.xhtml</to-view-id>
	    </navigation-case>
	    <navigation-case>
		<from-action>#{navigation2Ctrl.processPage2}</from-action>
		<from-outcome>page</from-outcome>
		<to-view-id>04/page2.xhtml</to-view-id>
	     </navigation-case>
	</navigation-rule>

</faces-config>
